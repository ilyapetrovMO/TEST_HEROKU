@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">

    <h1 class="display-4">Welcome</h1>
    <form method="GET" >
        <input name="createdAt"/>
        <button type="submit">Создать отчет</button>
    </form>

    <ul class="list-group">
        @foreach (IndexModel.ReportType rep in Model.Reports)
        {
            <il class="list-group-item">@rep.Name<text> </text>@rep.Balance</il>
        }
    </ul>
</div>
